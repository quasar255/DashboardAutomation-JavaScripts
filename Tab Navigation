
function BackToDashboard() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var sheet = ss.getSheetByName("Dashboard");
  ss.setActiveSheet(sheet).setActiveSelection("C1");
}

function JanData() {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var sheet = ss.getSheetByName("Jan");
  ss.setActiveSheet(sheet).setActiveSelection("C1");
}

